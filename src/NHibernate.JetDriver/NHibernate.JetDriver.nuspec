<?xml version="1.0"?>
<package >
  <metadata>
    <!-- Required elements-->

    <!-- The identifier that must be unique within the hosting gallery -->
    <id>Geomatics.IO.NHibernate.JetDriver</id>

    <!-- The package version number that is used when resolving dependencies -->
    <version>$version$</version>
    
     <!-- The description can be used in package manager UI. Note that the
            nuget.org gallery uses information you add in the portal. -->
    <description>The Jet driver and dialect for NHibernate.</description>

    <!-- Authors contain text that appears directly on the gallery -->
    <authors>Christian Junk, NHibernate.org</authors>

    <!-- Optional elements -->

    <title>Geomatics.IO.NHibernate.JetDriver</title>
     <!-- Owners are typically nuget.org identities that allow gallery
            users to easily find other packages by the same owners.  -->
    <owners>christianjunk, geomatics</owners>

    <!-- License and project URLs provide links for the gallery -->
    <projectUrl>https://github.com/geomatics-io/NHibernate.JetDriver</projectUrl>
    <licenseUrl>https://raw.githubusercontent.com/geomatics-io/NHibernate.JetDriver/master/LICENSE.md</licenseUrl>

    <!-- The icon is used in Visual Studio's package manager UI -->
    <iconUrl>https://github.com/geomatics-io/NHibernate.JetDriver/raw/master/NHibernate.JetDriver.png</iconUrl>

    <!-- If true, this value prompts the user to accept the license when
            installing the package. -->
    <requireLicenseAcceptance>false</requireLicenseAcceptance>

    <!-- Tags appear in the gallery and can be used for tag searches -->
    <tags>NHibernate jet driver csharp csharp-library dotnet</tags>

    <!-- Dependencies are automatically installed when the package is installed -->
    <dependencies>
        <group targetFramework=".NETFramework4.6.1">
            <dependency id="NHibernate" version="[5.0.0,5.2.7]" />
            <dependency id="log4net" version="[2.0.0,2.0.8]" />
        </group>
    </dependencies>
  </metadata>

  <files>
    <file src="..\..\.assets\icon.png" target="images\icon.png" />
    <file src="bin\Release\NHibernate.JetDriver.dll" target="lib\net461\NHibernate.JetDriver.dll" />
    <file src="bin\Release\NHibernate.JetDriver.pdb" target="lib\net461\NHibernate.JetDriver.pdb" />
    <file src="bin\Release\NHibernate.JetDriver.xml" target="lib\net461\NHibernate.JetDriver.xml" />
  </files>
</package>